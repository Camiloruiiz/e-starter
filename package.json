{
  "name": "e-starter-kit",
  "version": "1.0.0",
  "description": "Be a modern web developer with this ES6 Gulp workflow combined with Webpack",
  "main": "/tasks/index.js",
  "scripts": {
    "dev": "gulp --require babel-register --gulpfile tasks",
    "build": "NODE_ENV=production gulp build --require babel-register --gulpfile tasks",
    "push-s": "HOST=sitehost URL=site.com USER=site.user NODE_ENV=staging gulp deploy --require babel-register --gulpfile tasks",
    "push-p": "HOST=sitehost URL=staging.site.com USER=site.user NODE_ENV=production gulp deploy --require babel-register --gulpfile tasks",
    "wp": "gulp wp --require babel-register --gulpfile tasks",
    "seo": "URL=esquemacreativo.com gulp seo --require babel-register --gulpfile tasks",
    "webpack": "node -r babel-register ./node_modules/webpack/bin/webpack --config tasks/dev/webpack.js",
    "watch": "webpack && --watch"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "babelrc": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Camiloruiiz/e-starter"
  },
  "keywords": [],
  "author": "Camilo Ruiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Camiloruiiz/e-starterissues"
  },
  "homepage": "https://github.com/Camiloruiiz/e-starter#readme",
  "browserslist": [
    "> 0.3%",
    "last 2 versions",
    "ie >= 10",
    "ie_mob >= 10",
    "ff >= 30",
    "chrome >= 34",
    "Firefox ESR",
    "opera >= 23",
    "iOS >= 6",
    "android >= 4.4",
    "bb >= 10",
    "safari >= 5"
  ],
  "dependencies": {
    "bootstrap": "^4.1.0",
    "flickity": "^2.1.1",
    "jquery": "^3.2.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "apache-server-configs": "^2.15.0",
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.23.7",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "del": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.1.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^4.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
