{
  "name": "e-starter-kit",
  "version": "1.0.0",
  "description": "Be a modern web developer with this ES6 Gulp workflow combined with Webpack",
  "main": "/tasks/index.js",
  "scripts": {
    "webpack": "node -r @babel/register node_modules/.bin/webpack --config tasks/webpack.config.dev.js",
    "watch": "node -r @babel/register node_modules/.bin/webpack --config tasks/webpack.config.dev.js && --watch",
    "dev": "NODE_ENV=development node -r @babel/register node_modules/.bin/webpack-dev-server --config tasks/webpack.config.dev.js",
    "build": "NODE_ENV=production gulp --require @babel/register --gulpfile tasks",
    "push-s": "DEPLOY_ENV=staging gulp deploy --require @babel/register --gulpfile tasks",
    "push-p": "DEPLOY_ENV=production gulp deploy --require @babel/register --gulpfile tasks",
    "push-l": "DEPLOY_ENV=live gulp deploy --require @babel/register --gulpfile tasks",
    "wp": "NODE_ENV=wp gulp wp --require @babel/register --gulpfile tasks",
    "seo": "gulp seo --require @babel/register --gulpfile tasks",
    "clear:cache": "gulp clearCache --require @babel/register --gulpfile tasks",
    "clear": "gulp clear --require @babel/register --gulpfile tasks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Camiloruiiz/e-starter"
  },
  "keywords": [],
  "author": "Camilo Ruiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Camiloruiiz/e-starterissues"
  },
  "homepage": "https://github.com/Camiloruiiz/e-starter#readme",
  "browserslist": [
    "last 2 versions",
    "> 0.3%",
    "ie >= 10",
    "ie_mob >= 10",
    "Edge >= 12",
    "Explorer >= 10",
    "ff >= 30",
    "Chrome >= 45",
    "Firefox >= 38",
    "opera >= 30",
    "iOS >= 9",
    "android >= 4.4",
    "bb >= 10",
    "safari >= 5"
  ],
  "dependencies": {
    "bootstrap": "^4.1.3",
    "flickity": "^2.1.2",
    "flickity-bg-lazyload": "^1.0.0",
    "flickity-imagesloaded": "^2.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.5"
  },
